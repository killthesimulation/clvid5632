<% if(typeof errors != 'undefined') { %>
    <% errors.forEach(function(error) { %>
        <div class="error_msg">
            <div class="error_msg_icon"><i class="fas fa-exclamation-circle"></i></div>
            <div class="error_msg_text"> <%= error.msg %></div>
            <div class="error_msg_close" onclick="this.parentElement.style.display='none';"><i class="fas fa-times"></i></div>
        </div>
    <% }); %>
<% } %>



<% if(success_msg != ''){ %>
    

    <div class="success_msg">
        <div class="success_msg_icon"><i class="fas fa-check-circle"></i></div>
        <div class="success_msg_text">  <%= success_msg %></div>
        <div class="success_msg_close" onclick="this.parentElement.style.display='none';"><i class="fas fa-times"></i></div>
    </div>


   
<% } %>


<% if(error_msg != ''){ %>


    <div class="error_msg">
        <div class="error_msg_icon"><i class="fas fa-exclamation-circle"></i></div>
        <div class="error_msg_text"> <%= error_msg %></div>
        <div class="error_msg_close" onclick="this.parentElement.style.display='none';"><i class="fas fa-times"></i></div>
    </div>


   
<% } %>

<% if(error != ''){ %>


    <div class="error_msg">
        <div class="error_msg_icon"><i class="fas fa-exclamation-circle"></i></div>
        <div class="error_msg_text"><%= error %></div>
        <div class="error_msg_close" onclick="this.parentElement.style.display='none';"><i class="fas fa-times"></i></div>
    </div>


    
<% } %>
